// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

generator erd {
    provider = "prisma-erd-generator"
    output   = "./erd.svg"
    theme    = "forest"
}

datasource db {
    provider = "postgresql"
    url      = env("POSTGRES_URL")
}

model Test {
    id BigInt @id @db.BigInt

    @@map("tests")
}
